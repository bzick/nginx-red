<?xml version="1.0" encoding="UTF-8"?>
<urlrewrite>
    <langs>
        <lang>ru-ru</lang>
        <lang>zh-cn</lang>
        <lang>ja-jp</lang>
        <lang>fr-fr</lang>
        <lang>de-de</lang>
        <lang>es-es</lang>
        <lang>ko-kr</lang>
        <lang>pt-br</lang>
        <lang>tl-aa</lang>
        <prefix type="unlocalized">/store</prefix>
        <prefix type="unlocalized">/help</prefix>
    </langs>
    <config param="rules">urlrewrite.samples.xml</config>
    <config param="reload-timeout">30</config>
    <config param="debug">false</config>
</urlrewrite>